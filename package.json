{
    "name": "jsonplaceholder-tests",
    "version": "1.0.0",
    "description": "API tests for https://jsonplaceholder.typicode.com/",
    "main": "index.js",
    "scripts": {
        "lint": "eslint . --max-warnings=0 --ext .js,.jsx,.ts,.tsx --fix",
        "prettier": "prettier --write .",
        "ts-check": "./node_modules/typescript/bin/tsc --noEmit -p tsconfig.json",
        "pretest": "rm -f jest_html_reporters.html junit.xml",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git@github-gremjua:gremjua/jsonplaceholder-tests.git"
    },
    "author": "gremjua",
    "license": "ISC",
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --max-warnings=0 --fix",
        "*.{c,sc}ss": "stylelint",
        "**/*": "prettier --write --ignore-unknown"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.7",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "4.32.0",
        "@typescript-eslint/parser": "4.32.0",
        "eslint": "7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^25.2.4",
        "eslint-plugin-jsdoc": "^37.0.3",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "husky": "^7.0.4",
        "jest-html-reporters": "^2.1.6",
        "jest-junit": "^13.0.0",
        "lint-staged": "^11.2.6",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.7",
        "typedoc": "0.21.9",
        "typedoc-plugin-markdown": "3.10.4",
        "typescript": "4.4.3"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "email-validator": "^2.0.4",
        "jest": "27.3.1",
        "supertest": "^6.1.6"
    }
